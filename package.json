{
	"name": "jsdelivr-extension",
	"version": "0.1.0",
	"description": "A jsDelivr Extension for npm and GitHub",
	"scripts": {
		"lint": "npm run lint:css && npm run lint:js",
		"lint:css": "stylelint \"./src/**/*.css\"",
		"lint:js": "eslint --ext .js src test",
		"watch": "webpack --mode=development --watch",
		"build": "webpack --mode=production",
		"mocha": "NODE_ENV=test mocha --require @babel/register test/**/*.js",
		"test": "npm run lint && npm run mocha"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.6.2",
		"@babel/register": "^7.6.2",
		"@babel/runtime": "^7.6.2",
		"@martin-kolarik/eslint-config": "^2.0.0",
		"@martin-kolarik/stylelint-config": "^1.0.1",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"chai": "^4.2.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"eslint": "^6.4.0",
		"eslint-plugin-chai-expect": "^2.0.1",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"mini-css-extract-plugin": "^0.8.0",
		"mocha": "^6.2.1",
		"size-plugin": "^2.0.0",
		"stylelint": "^11.0.0",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.9"
	},
	"dependencies": {
		"copy-text-to-clipboard": "^2.1.1",
		"dom-chef": "^3.6.2",
		"dom-loaded": "^2.0.0"
	}
}